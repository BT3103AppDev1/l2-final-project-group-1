{
  "name": "vue-collapsed",
  "version": "1.1.2",
  "private": false,
  "description": "Dynamic CSS height transition from any to auto and vice versa for Vue 3. Accordion ready.",
  "keywords": [
    "vue",
    "vue-3",
    "vue-collapse",
    "vue-accordion",
    "collapse",
    "accordion",
    "vue-component"
  ],
  "homepage": "https://vue-collapsed.netlify.app/",
  "bugs": {
    "url": "https://github.com/smastrom/vue-collapsed/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smastrom/vue-collapsed.git"
  },
  "license": "MIT",
  "author": {
    "name": "Simone Mastromattei",
    "email": "smastrom@proton.me"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "lint-staged": {
    "*.{ts,vue}": "eslint --cache --fix",
    "*.{ts,vue,md}": "prettier --write"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.5.0",
    "cypress-wait-frames": "^0.9.4",
    "eslint": "^8.33.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "playwright-webkit": "^1.30.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.0.24"
  },
  "scripts": {
    "build": "rimraf dist && vite build",
    "build:app": "vite build --mode app",
    "dev": "vite",
    "test:chrome": "cypress run --component --browser chrome",
    "test:firefox": "cypress run --component --browser firefox",
    "test:webkit": "cypress run --component --browser webkit",
    "test": "test:chrome && test:firefox && test:webkit",
    "test:gui": "cypress open --component"
  }
}